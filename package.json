{
  "name": "wifiPunch",
  "version": "1.0.0",
  "description": "Automatic Punch for EvolvingWeb",
  "author": "Edouard Duplessis - FIXRS",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build --spa",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "json": "json-server --watch db.json --port 3004 --routes routes.json"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.3",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "nuxt": "^2.8.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "json-server": "^0.15.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.16.4"
  }
}
